{
  "name": "Fwall",
  "version": "1.0",
  "description": "Removes paywall on wsj.com and nytimes.com",
  "permissions": ["webRequest", "webRequestBlocking",
                  "*://*.nytimes.com/*", "*://*.wsj.com/*"],
  "background": {
    "scripts": ["background.js"]
  },

  "manifest_version": 2
}
